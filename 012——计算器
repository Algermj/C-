#include<stdio.h>
void main()
{
	while(1)
	{
		float  q,a,w;
		char i;
		scanf("%f%c%f",&q,&i,&w);

			switch(i)
			{
				case '+':	a=q+w;printf("%.1f%c%.1f=%.2f\n",q,i,w,a);break;
				case '-':	a=q-w;printf("%.1f%c%.1f=%.2f\n",q,i,w,a);break;
				case '*':	a=q*w;printf("%.1f%c%.1f=%.2f\n",q,i,w,a);break;
				case '/':	a=q/w;if (q==0){printf("输入错误!!\n");continue;}
									else {printf("%.1f%c%.1f=%.2f\n",q,i,w,a);break;}
			}
	}
}
